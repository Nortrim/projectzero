//title.styl
.title__circle
    margin: 0 auto
    height: 230px
    width: 230px
    border-radius: 50%
    background-color: $font-color
    border: 1px solid $font-color
    box-shadow: inset 0 0 0 14px white

.content-block__title
    font-size: 25px
    line-height: 5px
    text-align: center
    text-transform: uppercase
    padding-left 45px
    padding-bottom: 89px

.title__h1
    font-size: 55px
    font-weight: lighter
    line-height: 45px
    margin-bottom: 26px

.title__bold-name
    font-size: 60px
    line-height: 50px
    letter-spacing 4px
    font-weight: 900
